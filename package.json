{
  "name": "vibefoxes-vibe-coding-app",
  "version": "1.0.0",
  "description": "VibeFoxes - AI-powered code generator with a fox-inspired theme. By Jules François & Martz Cedric.",
  "main": "index.js",
  "scripts": {
    "start": "npm run build:css && npx http-server -p 8080 -o",
    "build:css": "npx tailwindcss -i ./styles.css -o ./dist/styles.css",
    "watch:css": "npx tailwindcss -i ./styles.css -o ./dist/styles.css --watch",
    "test": "node --check index.js",
    "lint": "eslint index.js",
    "lint:fix": "eslint index.js --fix",
    "check": "npm run test && npm run lint",
    "audit": "npm audit"
  },
  "keywords": [
    "ai",
    "code-generator",
    "openai",
    "gpt",
    "puter",
    "fox",
    "vibefoxes"
  ],
  "authors": [
    "Jules François",
    "Martz Cedric"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Cedric-Martz/ai_assignment.git"
  },
  "private": true,
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.17",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.56.0",
    "http-server": "^14.1.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17"
  }
}
