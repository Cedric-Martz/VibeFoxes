{
  "env": {
    "browser": true,
    "es2021": true,
    "jquery": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "globals": {
    "puter": "readonly",
    "tailwind": "readonly"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "no-eval": "warn",
    "no-undef": "warn",
    "prefer-const": "warn",
    "no-var": "warn"
  }
}
